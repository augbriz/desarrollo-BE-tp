#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/dist/lib/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/dist/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/dist/lib/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/dist/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules:/mnt/c/Users/Augusto/OneDrive - frro.utn.edu.ar/DSW-TP/tp/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/dist/lib/tsc-watch.js" "$@"
else
  exec node  "$basedir/../.pnpm/tsc-watch@7.0.0_typescript@5.8.3/node_modules/tsc-watch/dist/lib/tsc-watch.js" "$@"
fi
